---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <url>
    <loc>{{ site.url | absolute_url }}/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>1.0</priority>
  </url>

  {% for page in site.pages %}
    {% unless page.url contains '.xml' or page.url contains 'feed' or page.url contains '404' %}
    <url>
      <loc>{{ site.url | absolute_url }}{{ page.url }}</loc>
      <lastmod>{{ page.last_modified_at | default: site.time | date_to_xmlschema }}</lastmod>
      <changefreq>monthly</changefreq>
      <priority>0.8</priority>
    </url>
    {% endunless %}
  {% endfor %}

  {% for post in site.posts %}
    <url>
      <loc>{{ site.url | absolute_url }}{{ post.url }}</loc>
      <lastmod>{{ post.last_modified_at | default: post.date | date_to_xmlschema }}</lastmod>
      <changefreq>monthly</changefreq>
      <priority>0.7</priority>
    </url>
  {% endfor %}

  <!-- Company Pages -->
  <url>
    <loc>{{ site.url | absolute_url }}/companies/rk-oxygen/gorakhpur/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.9</priority>
  </url>

  <!-- Calculator Tools -->
  <url>
    <loc>{{ site.url | absolute_url }}/Calc/GST/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.6</priority>
  </url>

  <url>
    <loc>{{ site.url | absolute_url }}/Calc/LIQ/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.6</priority>
  </url>
</urlset>
